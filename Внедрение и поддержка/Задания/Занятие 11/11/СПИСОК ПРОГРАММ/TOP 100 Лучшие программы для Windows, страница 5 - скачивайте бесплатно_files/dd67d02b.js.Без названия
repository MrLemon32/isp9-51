function QuizzesShow(options){this.options=options=_extend({},options);var questionNum=0;var answerNum=0;var resultNum=0;var $this=this;var questionsSortOrder;var resultsSortOrder;var answersSortOrder={};var defaults={questionVisibility:null,ajaxLoader:null};for(var name in defaults){!(name in options)&&(options[name]=defaults[name]);}
this.filterQuestionTypesByQuizType=function(quizType){if(options.questionVisibility[quizType]){var oneVisArr=options.questionVisibility[quizType];$(".js-quiz-edit-question-type > option").each(function(){if($.inArray(this.value,oneVisArr)>-1){$(this).prop('disabled',false);}
else{$(this).prop('disabled',true);if(this.selected){this.selected=false;}}});}};this.onQuizTypeChanged=function(){this.filterQuestionTypesByQuizType(this.getActiveQuizType());};this.getActiveQuizType=function(){return $('select[name="quiz[quiz_type]"]').val();};this.showTrueAnswer=function(questionId){var questionTypeCname=$('#js-question-'+questionId).attr('data-question-type-cname');if(questionTypeCname==='truefalse'){$('#js-question-'+questionId+' .question-truefalse__answer').slideDown(250);}
if(questionTypeCname==='freetext'){$('#js-question-'+questionId+' .question-freetext__answer').slideDown(250);}
if(questionTypeCname==='freenum'){$('#js-question-'+questionId+' .question-freenum__answer').slideDown(250);}
if(questionTypeCname==='choice'||questionTypeCname==='multichoice'){$('#js-question-'+questionId+' .quizzes-list-answers__one').each(function(){var isTrue=this.getAttribute('data-answer-flag-is-true');if(isTrue==='1'){console.log(isTrue);var answerId=this.getAttribute('data-answer-id');$(this).addClass('quizzes-list-answers__one__is-true');$('#js-question-'+questionId+' #js-answer-'+answerId+' .quizzes-list-answers__title').addClass('quizzes-list-answers__is-true');}});}
$('#js-question-'+questionId+' .quizzes-list-questions__show-answer-button').hide();$('#js-question-'+questionId+' .quizzes-list-questions__hide-answer-button').show();$('select[name="quiz[quiz_type]"]').val();};this.hideTrueAnswer=function(questionId){var questionTypeCname=$('#js-question-'+questionId).attr('data-question-type-cname');if(questionTypeCname==='truefalse'){$('#js-question-'+questionId+' .question-truefalse__answer').slideUp(250);}
if(questionTypeCname==='freetext'){$('#js-question-'+questionId+' .question-freetext__answer').slideUp(250);}
if(questionTypeCname==='freenum'){$('#js-question-'+questionId+' .question-freenum__answer').slideUp(250);}
if(questionTypeCname==='choice'||questionTypeCname==='multichoice'){$('#js-question-'+questionId+' .quizzes-list-answers__one').each(function(){var isTrue=this.getAttribute('data-answer-flag-is-true');if(isTrue==='1'){var answerId=this.getAttribute('data-answer-id');$(this).removeClass('quizzes-list-answers__one__is-true');$('#js-question-'+questionId+' #js-answer-'+answerId+' .quizzes-list-answers__title').removeClass('quizzes-list-answers__is-true');}});}
$('#js-question-'+questionId+' .quizzes-list-questions__hide-answer-button').hide();$('#js-question-'+questionId+' .quizzes-list-questions__show-answer-button').show();};this.showAllTrueAnswers=function(){$(".quizzes-list-questions__one").each(function(){var qId=this.getAttribute('data-question-id');$this.showTrueAnswer(qId);});};this.hideAllTrueAnswers=function(){$(".quizzes-list-questions__one").each(function(){var qId=this.getAttribute('data-question-id');$this.hideTrueAnswer(qId);});};this.attachQuizEvents=function(){$('.js-show-all-answers').on("click",function(){$this.showAllTrueAnswers();});$('.js-hide-all-answers').on("click",function(){$this.hideAllTrueAnswers();});};this.attachQuestionsEvents=function(onlyInEmement){var selector='';if(typeof onlyInEmement!=='undefined'){selector=onlyInEmement+' ';}
$(selector+'.js-show-answer-button').on("click",function(){$this.showTrueAnswer($(this).parents(".quizzes-list-questions__one").attr('data-question-id'));});$(selector+'.js-hide-answer-button').on("click",function(){$this.hideTrueAnswer($(this).parents(".quizzes-list-questions__one").attr('data-question-id'));});};this.attachAnswersEvents=function(onlyInEmement){var selector='';if(typeof onlyInEmement!=='undefined'){selector=onlyInEmement+' ';}};this.attachResultsEvents=function(onlyInEmement){var selector='';if(typeof onlyInEmement!=='undefined'){selector=onlyInEmement+' ';}};this.attachQuizEvents();this.attachQuestionsEvents();this.attachAnswersEvents();this.attachResultsEvents();};